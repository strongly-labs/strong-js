{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.build.json --verbose --noClean",
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepublish": "npm run build",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@strongly/admin",
  "author": "Zuhair Naqvi",
  "module": "dist/admin.esm.js",
  "size-limit": [
    {
      "path": "dist/admin.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/admin.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@size-limit/preset-small-lib": "^5.0.1",
    "@storybook/addon-essentials": "^6.3.5",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.5",
    "@storybook/addons": "^6.3.5",
    "@storybook/react": "^6.3.5",
    "@types/babel__core": "^7.1.15",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-is": "^17.0.2",
    "@types/react-table": "^7.7.2",
    "@types/react-timeago": "^4.1.3",
    "@types/storybook__addon-info": "^5.2.4",
    "babel-loader": "^8.2.2",
    "husky": "^7.0.1",
    "react-is": "^17.0.2",
    "size-limit": "^5.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0"
  },
  "dependencies": {
    ".strongly": "link:../../.strongly",
    "evergreen-ui": "^6.2.0",
    "match-sorter": "^6.3.0",
    "react-hook-form": "^7.11.1",
    "react-table": "^7.7.0",
    "react-timeago": "^6.2.1",
    "swr": "^0.5.6"
  }
}
