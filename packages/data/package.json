{
  "name": "@strong-js/data",
  "version": "0.1.6",
  "description": "Strong-js Data",
  "license": "MIT",
  "author": "Strongly Labs",
  "main": "dist/index.js",
  "scripts": {
    "type-check": "tsc --project ./tsconfig.build.json",
    "type-check:watch": "yarn type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly --project ./tsconfig.build.json",
    "build:js": "rollup -c",
    "build:watch": "rollup -w -c",
    "build": "yarn build:types && yarn build:js",
    "prepublish": "yarn build"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongly-labs/strong-js.git",
    "directory": "packages/data"
  },
  "dependencies": {
    "@prisma/client": "~2.29.0",
    "prisma": "~2.29.0",
    "tslib": "^2.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {}
}
