{
  "name": "@strong-js/data",
  "version": "0.0.3",
  "description": "Strong-js Data",
  "author": "Strong-js Labs",
  "main": "dist/index.js",
  "scripts": {
    "type-check": "tsc --noEmit --project ./tsconfig.build.json",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly --project ./tsconfig.build.json",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:js",
    "prepublish": "npm run build",
    "gen": "npx prisma generate",
    "kill": "sudo kill -9 $(sudo lsof -t -i:5432)",
    "up": "docker-compose up",
    "down": "docker-compose down",
    "push": "npx prisma db push",
    "migrate": "npx prisma migrate dev"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "dependencies": {
    "@prisma/client": "~2.27.0",
    "prisma": "~2.27.0",
    "tslib": "^2.3.0",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.14.7"
  },
  "devDependencies": {
    "@strong-js/generate": "*",
    "@types/graphql-fields": "^1.3.4",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.8.4",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "prisma": {
    "schema": "../../backend/schema.prisma"
  }
}
