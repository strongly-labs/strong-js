{
  "name": "@strongly/data",
  "version": "0.0.1",
  "description": "Strongly Data",
  "author": "Strongly Pty Ltd",
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.build.json --verbose --noClean",
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepublish": "npm run build",
    "gen": "npx prisma generate",
    "up": "docker-compose up",
    "down": "docker-compose down",
    "push": "npx prisma db push",
    "migrate": "npx prisma migrate dev"
  },
  "module": "dist/generate.esm.js",
  "files": [
    "README.md",
    "dist"
  ],
  "alias": {
    ".strongly": "../../.strongly"
  },
  "dependencies": {
    "@prisma/client": "^2.26.0",
    "prisma": "^2.26.0",
    "tslib": "^2.3.0",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.14.6"
  },
  "devDependencies": {
    "@strongly/generate": "file:../generate",
    "@types/graphql-fields": "^1.3.4",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.8.4",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.10.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "prisma": {
    "schema": "../../backend/schema.prisma"
  }
}
