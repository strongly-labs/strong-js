{
  "name": "@strong-js/cli",
  "version": "0.1.2",
  "license": "MIT",
  "description": "Strong-js CLI",
  "author": "Strongly Labs",
  "main": "dist/index.js",
  "module": "dist/cli.esm.js",
  "bin": {
    "strong-js": "./dist/index.js"
  },
  "scripts": {
    "type-check": "tsc --noEmit --project ./tsconfig.build.json",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly --project ./tsconfig.build.json",
    "build:js": "rollup -c",
    "build:watch": "rollup -w -c",
    "build": "npm run build:types && npm run build:js",
    "prepublish": "npm run build"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongly-labs/strong-js.git",
    "directory": "packages/cli"
  },
  "dependencies": {
    "abstract-syntax-tree": "^2.20.0",
    "chalk": "^4.1.1",
    "chance": "^1.1.7",
    "enquirer": "^2.3.6",
    "envfile": "^6.15.0",
    "execa": "^5.1.1",
    "find-yarn-workspace-root": "^2.0.0",
    "fs-extra": "^10.0.0",
    "ora": "^5.4.1",
    "regenerator-runtime": "^0.13.9",
    "replace-in-files": "^3.0.0",
    "sade": "^1.7.4",
    "tslib": "^2.3.0",
    "type-fest": "^1.2.2",
    "unique-names-generator": "^4.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
    "@types/fs-extra": "^9.0.12",
    "@types/sade": "^1.7.3"
  }
}
